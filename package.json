{
  "name": "karin-plugin-teamspeak3",
  "version": "1.2.1",
  "description": "karin 的 teamspeak3 插件",
  "homepage": "https://github.com/jacksixth/karin-plugin-teamspeak3",
  "bugs": {
    "url": "https://github.com/jacksixth/karin-plugin-teamspeak3/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacksixth/karin-plugin-teamspeak3.git"
  },
  "author": "jack",
  "type": "module",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "pub": "npm publish --access public",
    "clean": "npm lib/cli/pr.js clean",
    "pr": "node lib/cli/pr.js"
  },
  "devDependencies": {
    "@types/node": "^20.17.8",
    "eslint": "^9.7.0",
    "neostandard": "^0.11.9",
    "node-karin": "^1.3.13",
    "ts3-nodejs-library": "^3.5.1",
    "tsc-alias": "^1.8.10",
    "tsx": "^3.12.10",
    "typescript": "^5.5.3"
  },
  "karin": {
    "apps": [
      "lib/apps"
    ],
    "ts-apps": [
      "src/apps"
    ],
    "static": [
      "resources"
    ],
    "files": [
      "config",
      "data",
      "resources"
    ],
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "lib/**/*",
      "@karinjs/**/*"
    ]
  },
  "files": [
    "/lib/**/*.js",
    "/lib/**/*.d.ts",
    "/config/*.yaml",
    "/config/*.json",
    "resources",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
